<h2>Create a Song</h2>
<%= form_for @song do |s| %>
  <div class="field">
    <%= s.text_field :title %>
  </div>
  <div class="field">
    <%= s.check_box :released %>
  </div>
  <div class="field">
    <%= s.number_field :release_year %>
  </div>
  <div class="field">
    <%= s.text_field :artist_name %>
  </div>
  <div class="field">
    <%= s.text_field :genre %>
  </div>
<% end %>
